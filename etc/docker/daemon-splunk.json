{
    "data-root": "/data/docker",
    "hosts": ["unix:///run/docker.sock"],
    "builder": {
        "Entitlements": {
            "security-insecure": true
        }
    },
    "log-driver": "splunk",
    "log-opts": {
        "splunk-token": "11111111-1111-1111-1111-111111111111",
        "splunk-url": "http://splunk:8088",
        "splunk-insecureskipverify": "true",
        "splunk-verify-connection": "false",
        "splunk-gzip": "false",
        "splunk-gzip-level": "0",
        "splunk-format": "json",
        "tag": "{{.Name}}/{{.ID}}",
        "labels": "container_name,container_id,service",
        "env": "DOJO_HOST,DOJO_ENV,WORKSPACE_NODE"
    }
}
